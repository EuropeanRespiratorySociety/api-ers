{
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://api.ersnet.org",
      "subject": "anonymous",
      "issuer": "European Respiratory Society",
      "algorithm": "HS256",
      "expiresIn": "30d"
    },
    "local": {
      "entity": "user",
      "service": "users",
      "usernameField": "email",
      "passwordField": "\\password",
      "idField": "_id"
    },
    "localUsername": {
      "name": "local-username",
      "entity": "user",
      "service": "users",
      "usernameField": "username",
      "passwordField": "\\password",
      "idField": "_id"
    }
  },
  "redisCache" : {
    "defaultDuration": 3600,
    "removePathFromCacheKey": true,
    "env": "NODE_ENV"
  },
  "mongodb": "MONGODB"
}
