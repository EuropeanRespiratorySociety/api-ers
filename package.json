{
  "name": "ers-api",
  "description": "The ERS API provides an API to retrieve content and facilitate login with myERS",
  "version": "0.1.2",
  "homepage": "https://api.ersnet.org",
  "main": "src",
  "keywords": [
    "feathers",
    "Cloud CMS"
  ],
  "author": {
    "name": "idealley",
    "email": "samuelpouyt@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EuropeanRespiratorySociety/api-ers.git"
  },
  "bugs": {
    "url": "https://github.com/EuropeanRespiratorySociety/api-ers/issues"
  },
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "test": "npm run eslint && nyc --reporter=lcov --reporter=text mocha --compilers js:babel-core/register --colors ./test/ --recursive",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "node src/",
    "mocha": "mocha test/ --recursive",
    "mocha:filters": "mocha test/services/filters.test.js",
    "mocha:hooks": "mocha test/hooks --recursive --watch",
    "mocha:helpers": "mocha test/helpers --recursive --watch",
    "mocha:watch": "mocha test/ --recursive --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.1",
    "@feathersjs/authentication-client": "^1.0.2",
