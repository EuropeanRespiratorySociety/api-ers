{
  "name": "ers-api",
  "description": "The ERS API provides an API to retrieve content and facilitate login with myERS",
  "version": "0.1.2",
  "homepage": "https://api.ersnet.org",
  "main": "src",
  "keywords": [
    "feathers",
    "Cloud CMS"
  ],
  "author": {
    "name": "idealley",
    "email": "samuelpouyt@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EuropeanRespiratorySociety/api-ers.git"
  },
  "bugs": {
    "url": "https://github.com/EuropeanRespiratorySociety/api-ers/issues"
  },
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "test": "npm run eslint && nyc --require babel-core/register npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "node src/",
    "mocha": "mocha ./test/ --recursive --exit",
    "mocha:filters": "mocha test/services/filters.test.js",
    "mocha:class": "mocha test/classes",
    "mocha:hooks": "mocha ./test/hooks --recursive --exit",
    "mocha:webhook": "mocha test/webhook --recursive --watch",
    "mocha:helpers": "mocha test/helpers --recursive",
    "mocha:watch": "mocha test/ --recursive --watch",
    "mocha:single": "mocha test/services/calendar.test.js",
    "coverage": "nyc report --reporter=text",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.5",
    "@feathersjs/authentication-client": "^1.0.2",
    "@feathersjs/authentication-jwt": "^2.0.0",
    "@feathersjs/authentication-local": "^1.1.3",
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.2",
    "@feathersjs/feathers": "^3.1.4",
    "@feathersjs/rest-client": "^1.3.4",
    "@feathersjs/socketio": "^3.2.1",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.4.1",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "elasticsearch": "^15.0.0",
    "ers-utils": "^0.2.4",
    "feathers-authentication-hooks": "^0.1.7",
    "feathers-hooks-common": "^4.11.1",
    "feathers-hooks-rediscache": "^1.1.2",
    "feathers-mongoose": "^6.1.1",
    "feathers-swagger": "^0.6.0",
    "gitana": "^1.0.256",
    "graphql": "^0.13.2",
    "graphql-server-express": "^1.3.6",
    "graphql-tools": "^2.24.0",
    "helmet": "^3.12.0",
    "mongodb": "^3.0.7",
    "mongoose": "^5.0.17",
    "proxy-agent": "^3.0.0",
    "redis": "^2.8.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.5.0",
    "validator": "^9.4.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "istanbul": "^0.4.5",
    "istanbul-middleware": "^0.2.2",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "request": "^2.85.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
