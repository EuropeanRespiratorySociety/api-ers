    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.3",
    "eslint": "^4.17.0",
    "istanbul": "^0.4.5",
    "istanbul-middleware": "^0.2.2",
    "mocha": "^3.5.3",
    "nyc": "^11.5.0",
